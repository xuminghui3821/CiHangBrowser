<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1dip" />
            <gradient
                android:angle="270"
                android:centerColor="@color/xian"
                android:centerY="0.75"
                android:endColor="@color/xian"
                android:startColor="@color/xian" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/base_color"
                    android:centerY="0.75"
                    android:endColor="@color/base_color"
                    android:startColor="@color/base_color" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="1dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#fed7ec"
                    android:centerY="0.75"
                    android:endColor="#fed7ec"
                    android:startColor="#fed7ec" />
            </shape>

        </clip>

    </item>
</layer-list>